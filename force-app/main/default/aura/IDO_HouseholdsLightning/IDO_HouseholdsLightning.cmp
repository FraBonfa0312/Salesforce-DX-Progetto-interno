<aura:component implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction"  access="global" controller="IDO_HouseholdRelationshipCtrl">
		
    
    	<aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
     	
        <aura:attribute name="accountName" type="String" default="Claire Adams"/>
    	<aura:attribute name="recordId" type="Id" />
    	<aura:attribute name="thisAccount" type="Account" />
     	<aura:attribute name="lstKey" type="List"/>
   		<aura:attribute name="fullMap" type="Map"/>
      	<aura:attribute name="householdContacts" type="Contact[]" />
       
    

    
  		<article class="slds-card">
		<div class="slds-card__header slds-grid">
			<header class="slds-media slds-media_center slds-has-flexi-truncate">
				<div class="slds-media__figure">
					<span class="slds-icon_container slds-icon-standard-contact" title="description of icon when needed">
	
                        	 <lightning:icon iconName="standard:client" size="small"/>
                        
					</span>
				</div>
				<div class="slds-media__body">
                    <h2>
                        <a href="javascript:void(0);" class="slds-card__header-link slds-truncate" title="">
                            <span class="slds-text-heading_small">{!v.thisAccount.Name}</span>
                        </a>
                    </h2>
                </div>
            </header>
			<div class="slds-no-flex">

			</div>
		</div>

       <aura:iteration items="{!v.lstKey}" var="key" >
    		<c:IDO_RelationshipGroupMap map="{!v.fullMap}" key="{!key}"/>
   		</aura:iteration>
     	</article>



</aura:component>