<aura:component implements="force:hasRecordId,flexipage:availableForAllPageTypes">
    
	<aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
	<lightning:flow aura:id="flowData" onstatuschange="{!c.handleStatusChange}"/>
    
    <div aura:id="Transactions" class="slds-grid slds-wrap">
        <div class="slds-size--1-of-1 slds-p-around_large">
            <h1>Select a transaction</h1>
        </div>
        <div class="slds-size--1-of-2 slds-p-around_medium">
            <a href="">
                <img src="{!$Resource.Teller2Transfer}"/>
            </a>
        </div>
        <div class="slds-size--1-of-2 slds-p-around_medium">
            <a href="">
                <img src="{!$Resource.Teller2Withdrawal}"/>
            </a>
        </div>
        <div class="slds-size--1-of-2 slds-p-around_medium">
            <a href="">
                <img src="{!$Resource.Teller2CashCheck}"/>
            </a>
        </div>
        <div class="slds-size--1-of-2 slds-p-around_medium">
            <a href="">
                <img src="{!$Resource.Teller2Deposit}"/>
            </a>
        </div>
        <div class="slds-size--1-of-2 slds-p-around_medium">
            <a href="">
                <img src="{!$Resource.Teller2AcctBalance}"/>
            </a>
        </div>
        <div class="slds-size--1-of-2 slds-p-around_medium">
            <a href="">
                <img src="{!$Resource.Teller2Lockbox}"/>
            </a>
        </div>
        <div class="slds-size--1-of-2 slds-p-around_medium">
            <a href="">
                <img src="{!$Resource.Teller2Statements}"/>
            </a>
        </div>
        <div class="slds-size--1-of-2 slds-p-around_medium">
            <a onclick="{!c.waiveFee}">
                <img src="{!$Resource.Teller2WaiveFee}"/>
            </a>
        </div>
    </div>
</aura:component>