                                <lightning:layout>
                                    <lightning:layoutItem size="2">
                                        <div class="div-checkbox">
                                            <img class="{!item.coverages[0].Covered__c == true ? 'active': ''}" src="{!$Resource.Q_GWIconCheck_White}" onclick="{!c.handleCheckboxClick}"/>
