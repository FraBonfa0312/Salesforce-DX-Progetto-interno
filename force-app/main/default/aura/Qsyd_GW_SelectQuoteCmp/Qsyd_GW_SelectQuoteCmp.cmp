    <aura:attribute name="programName1" type="String" default="Basic Program"/>
    <aura:attribute name="programName2" type="String" default="Premium Program"/>
    <aura:attribute name="programName3" type="String" default="Standard Program"/>
    <aura:attribute name="programPrice1" type="String" default="20.67"/>
    <aura:attribute name="programPrice2" type="String" default="182.00"/>
    <aura:attribute name="programPrice3" type="String" default="115.17"/>
    <aura:attribute name="currencySymbol" type="String" default="$"/>
    <aura:attribute name="programList" type="List" default=""/>
    <aura:attribute name="selectedProgramIdx" type="String" default=""/>
