                        <lightning:button label="Pay in Full" name="payInFull" aura:id="QuoteType" onclick="{!c.handleToggleBtnClick}" />
                    </lightning:buttonGroup>
                </div>
            </lightning:layoutItem>
            <lightning:layoutItem size="12" class="slds-m-top_small">
                <!--HEADERROW  OLD-->
                <lightning:layout multipleRows="true" class="tableHeaderRow slds-text-align--center">
                    <aura:iteration items="" var="program" indexVar="idx">
                    <lightning:layoutItem size="4">
                        <!--CELL1-->
                        <div class="tableHeaderCells">
                            <lightning:layout>
                                <lightning:layoutItem size="12">
                                    <div class="cellProgramLabel slds-m-bottom_xx-small">
                                        {!program.name}
                                    </div>
                                    <div class="cellProgramPrice">
                                        <span>{!v.currencySymbol}</span>
                                        <span id="{!program.name}" class="price">{!program.price}</span>
                                        <aura:if isTrue="{!program.numOfMonth == 1}">
                                            <span class="month"> / mo.</span>
                                        </aura:if>
                                    </div>
                                    <div class="cellProgramBtn">
                                        <lightning:button label="Buy Now" variant="brand" name="{!program.index}" onclick="{!c.handleBuyNow}"/>
                                    </div>
                                </lightning:layoutItem>
                            </lightning:layout>
                        </div>
                    </lightning:layoutItem>
                 </aura:iteration>
                </lightning:layout>
            </lightning:layoutItem>
            <!--HEADERROW-->

            <lightning:layoutItem size="12" class="slds-m-top_small">
